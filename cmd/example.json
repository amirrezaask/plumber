{
    "from": {
        "type": "nats-streaming",
        "args": {
            "url": "localhost:4222",
            "subject": "plumber"
        }
    },
    "to": {
        "type": "nats-streaming",
        "args": {
            "url": "localhost:4222",
            "subject": "plumber-out"
        }
    },
    "checkpoint": {
        "type": "time-based",
        "args": {
            "interval": 2
        }
    },
    "state": {
        "type": "redis",
        "args": {
            "host": "localhost",
            "port": "6379",
            "database": "0"
        }
    },
    "pipeline": [
        {
            "path": "grep awesome",
            "needs_state": false
        },
        {
            "path": "our bin",
            "needs_state": true
        }
    ]
}
